<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.lbsbackend.mapper.LibraryMapper">
    <select id="queryById" resultType="com.example.lbsbackend.entity.Library">
        SELECT
        ID AS id,
        NAME AS name
        FROM LIBRARY
        WHERE ID = #{id}
    </select>

</mapper>